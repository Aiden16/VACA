    <% layout('layouts/boilerplate') %>
    <h3>All places</h3>
        <% for(let place of places){ %>
            <div class="card" mb-3>
                <div class="row">
                    <div class="col-md-4">
                        <img  class='img-fluid'src="<%=place.images[0].url%>" alt="">
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <h5 class="card-title">
                                <%=place.title%>
                            </h5>
                            <p class="card-text">
                                <%=place.description%>
                            </p>
                            <p class="card-text">
                                <small class="text-muted"><%=place.location%></small>
                            </p>
                            <a href="/places/<%=place.id%>" class="btn btn-primary">View <%=place.title%></a>
                        </div>
                    </div>
                </div>
            </div>
        <% } %>

